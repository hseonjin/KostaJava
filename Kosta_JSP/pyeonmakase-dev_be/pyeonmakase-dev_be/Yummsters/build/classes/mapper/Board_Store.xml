<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.board_store">
    <insert id="insertBoard_store" parameterType="Board_Store">
        <![CDATA[
        insert into board_store (board_id, store_category_id) values(#{board_id}, #{store_category_id})
        ]]>
    </insert>
</mapper>